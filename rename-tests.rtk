#lang racket
(require "language.rkt")
(require (rename-in "rename.rkt" (rename rename:rename)))
(require "utils.rkt")

(define rename
  (lambda(exp)
    (parse exp
           (lambda(pt)
             (rename:rename pt))
           (lambda()
             #f))))

(rename '((lambda(x)(lambda(y)( + x y))) (+ x y)))
